{
  "compilerOptions": {
    "outDir":"dist",
    "module": "commonjs",
    "target": "es2019",
    "esModuleInterop": true,
    "sourceMap": true,
    "rootDir": "src",
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"]
    }       
  },
  "include": [
    "src"
  ],
  "exclude": [
    "**/node_modules",
    "node_modules",
    "dist",
    "coverage",
    "data",
    "dist",
  ]
}
